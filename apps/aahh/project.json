{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "options": {
        "outputPath": "dist/apps/aahh"
      },
      "configurations": {
        "development": {
          "minify": false,
          "mode": "development",
          "sourceMap": true,
          "ssrManifest": false
        },
        "production": {
          "minify": true,
          "mode": "production",
          "sourceMap": "hidden",
          "ssrManifest": true
        }
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "wrangler pages publish dist/apps/aahh --project-name aahh"
      }
    },
    "serve": {
      "executor": "@nrwl/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "aahh:build"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p apps/aahh/tsconfig.json"
      }
    }
  },
  "tags": []
}
